<%- include('../Partials/head.html')  %> 
    <div class="container">
        <div class="row p-5">
            <div class="shadow col-5 mx-auto">
                <h2 class="text-primary text-center display-4 fw-bold">View Post </h2>
                <div class="card p-2 ">
                    <img style="width:30%;height:6vh;" src="<%= result.file %>" alt="">
                <h1 class="text-center fw-bold h4 mx-auto"> Category: <%= result.category %> </h1>
                <h1 class="text-center fw-bold h4 mx-auto">Title: <%= result.title %> </h1>
                <h1 class="text-center fw-bold h4 mx-auto">More: <%= result.more %> </h1>
                <h1 class="text-center fw-bold mx-auto mb-3">Date: <%= result.date %> </h1>
                </div>
            </div>
            <a href="/all-post"><button class="btn btn-primary" >Back to Post</button></a>
        </div>
    </div>
<%- include('../Partials/bottom.html')  %> 